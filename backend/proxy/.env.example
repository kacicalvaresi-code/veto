# Veto Backend Proxy - Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=production

# Call Control API Configuration
# Sign up at https://www.callcontrol.com/developers/
CALL_CONTROL_API_KEY=your_api_key_here

# CORS Configuration (Mobile App URLs)
ALLOWED_ORIGINS=https://veto.app,http://localhost:8081

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
